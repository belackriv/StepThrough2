<button name="print" type="button" class="button">Print</button>
<button name="back" type="button" class="button">Back</button>
<div data-ui="printContainer">
	<h3 class="title is-3">Bin #{{id}} : {{name}}</h3>
	{{#if travelerIdCount}}
		<h5 class="subtitle is-5">Has {{travelerIdCount}} Traveler Ids</h5>
		<div class="box">
			<div class="is-pulled-left">
				<ul>
				{{#each tidColumns.[0]}}
					<li>
						{{barcodeHtml this}}
					</li>
				{{/each}}
				</ul>
			</div>
			<div class="is-pulled-left">
				<ul>
				{{#each tidColumns.[1]}}
					<li>
						{{barcodeHtml this}}
					</li>
				{{/each}}
				</ul>
			</div>
			<div class="is-pulled-left">
				<ul>
				{{#each tidColumns.[2]}}
					<li>
						{{barcodeHtml this}}
					</li>
				{{/each}}
				</ul>
			</div>
			<div class="is-clearfix"></div>
		</div>
	{{/if}}
	{{#if salesItemCount}}
		<h5 class="subtitle is-5">Has {{salesItemCount}} Sales Items</h5>
		<div class="box">
			<div class="is-pulled-left">
				<ul>
				{{#each siColumns.[0]}}
					<li>
						{{barcodeHtml this}}
					</li>
				{{/each}}
				</ul>
			</div>
			<div class="is-pulled-left">
				<ul>
				{{#each siColumns.[1]}}
					<li>
						{{barcodeHtml this}}
					</li>
				{{/each}}
				</ul>
			</div>
			<div class="is-pulled-left">
				<ul>
				{{#each siColumns.[2]}}
					<li>
						{{barcodeHtml this}}
					</li>
				{{/each}}
				</ul>
			</div>
			<div class="is-clearfix"></div>
		</div>
	{{/if}}
	</div>
</div>