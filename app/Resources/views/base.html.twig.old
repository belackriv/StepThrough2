<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}StepThrough{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.css') }}"/>
            <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap-theme.css') }}"/>
            <link rel="stylesheet" href="{{ asset('assets/vendor/spin/stylesheets/jquery.spin.css') }}"/>
            <link rel="stylesheet" href="{{ asset('assets/css/stepthrough.css') }}"/>
        {% endblock %}
        {% block favicon %}<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />{% endblock %}
        {% block requirejs %}
            <script data-main="{{ asset('assets/js/require_main.js') }}" src="{{ asset('assets/vendor/requirejs/require.js') }}"></script>
        {% endblock %}
    </head>
    <body>
    	<div id="content">{% block content %}{% endblock %}</div>
    </body>
</html>
